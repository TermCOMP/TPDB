(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f3))
(VAR A B C D)
(RULES
  f3(A,B,C) -> Com_1(f2(A,B,C))
  f2(A,B,C) -> Com_1(f2(-1 + A,D,C)) :|: D >= 1 && A >= 0
  f2(A,B,C) -> Com_1(f2(-1 + A,D,C)) :|: 0 >= D + 1 && A >= 0
  f2(A,B,C) -> Com_1(f2(A,0,C)) :|: A >= 0
  f2(A,B,C) -> Com_1(f300(A,B,D)) :|: 0 >= A + 1
)
