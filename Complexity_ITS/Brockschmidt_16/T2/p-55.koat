(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f2))
(VAR A B C D)
(RULES
  f300(A,B,C) -> Com_1(f300(-99 + A,0,C)) :|: 0 >= A + 1 && B + 1 = 0
  f300(A,B,C) -> Com_1(f300(1 + A,1 + B,C)) :|: 0 >= A + 1 && B >= 0
  f300(A,B,C) -> Com_1(f300(1 + A,1 + B,C)) :|: 0 >= A + 1 && 0 >= 2 + B
  f300(A,B,C) -> Com_1(f1(A,B,D)) :|: A >= 0
  f2(A,B,C) -> Com_1(f300(A,B,C))
)
