(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f2))
(VAR A B C D)
(RULES
  f300(A,B,C) -> Com_1(f300(-1 + A,-2 + A,C)) :|: A >= 1 && B + A >= 1 && B >= 1
  f300(A,B,C) -> Com_1(f1(A,B,D)) :|: A >= 1 && 0 >= B + A && B >= 1
  f300(A,B,C) -> Com_1(f1(A,B,D)) :|: B >= 1 && 0 >= A
  f300(A,B,C) -> Com_1(f1(A,B,D)) :|: 0 >= B
  f2(A,B,C) -> Com_1(f300(A,B,C))
)
