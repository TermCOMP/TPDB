(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f2))
(VAR A B C D E)
(RULES
  f2(A,B,C,D) -> Com_1(f300(A,B,C,D))
  f300(A,B,C,D) -> Com_1(f300(1 + A,B,E,D)) :|: E >= 1 && B >= 1 + A
  f300(A,B,C,D) -> Com_1(f300(1 + A,B,E,D)) :|: 0 >= E + 1 && B >= 1 + A
  f300(A,B,C,D) -> Com_1(f300(A,B,0,D)) :|: B >= 1 + A
  f300(A,B,C,D) -> Com_1(f1(A,B,C,E)) :|: A >= B
)
